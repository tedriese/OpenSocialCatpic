  <script src="~/content/gadgets/compliance/common/opensocialtestrunner.js"></script>
  <script src="~/content/gadgets/compliance/common/restfultestrunner.js"></script>


  <script src="~/content/gadgets/compliance/common/activitiessuite.js"></script>
